<script>
 <%= erb :require_js %>
  function loadScripts(success, failure) {
    requirejs.config({
      shim: {
          'jquery.mousewheel': ['jquery'],
          'jquery.hammer': ['jquery'],
          'jquery.hotkeys': ['jquery']
      },
      baseUrl: "/",
      paths: {
        "jquery": '<%=  "#{settings.static_host}/jquery-2.0.2.min" %>',
        "underscore": '<%=  "#{settings.static_host}/underscore-1.4.4.min" %>',
        "jquery.mousewheel": '<%=  "#{settings.static_host}/jquery.mousewheel-3.1.3" %>',
        "color": '<%=  "#{settings.static_host}/color-0.4.1.min" %>',
        "kinetic": '<%=  "#{settings.static_host}/kinetic-v4.5.4.min" %>',
        "jquery.hammer": '<%=  "#{settings.static_host}/jquery.hammer.min" %>',
        "jquery.hotkeys": '<%=  "#{settings.static_host}/jquery.hotkeys" %>'
      },
      waitSeconds: 120
    });
    requirejs(
      ["jquery", "underscore"],
      function () {
        requirejs(
          ["jquery.mousewheel", "color", "kinetic", "jquery.hammer", "jquery.hotkeys"],
          function () {
            requirejs(
              [
                "<%=settings.cache_prevention_key%>/mapjs-compiled",
                "<%=settings.cache_prevention_key%>/s3-adapter",
                "<%=settings.cache_prevention_key%>/file-system-map-source",
                "<%=settings.cache_prevention_key%>/retriable-map-source-decorator"
            ], success, failure);
          },
          failure
        );
      },
      failure
    );
  }
</script>
