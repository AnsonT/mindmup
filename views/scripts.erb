<script src="<%=settings.static_host%>/external.js" ></script>
<% if production? then %>
    <script src="<%=settings.public_url%>/mm-compiled.min.js" ></script>
<% else %>
      <script src="/mm.js" ></script>
      <script src="/mapjs-compiled.js" ></script>
      <% development_lib.each do |script| %>
          <script src="<%=script%>" ></script>
      <% end %>
      <script src="/main.js" ></script>
<% end%>
<script>
  function loadScripts(success, failure) {
    if (window.MAPJS && window.jQuery) {
      window.mmtimestamp.log('remote scripts loaded');
      success();
    } else {
      failure ();
    }
  }
</script>
